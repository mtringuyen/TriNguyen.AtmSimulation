@{
    ViewData["Title"] = "Account List";
}
@model IEnumerable<TriNguyen.AtmSimulation.Web.ApiModels.AccountDTO>
<h2>Accounts</h2>
<a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="AddAccount">Add new Account</a>
<ul>
    @foreach (var item in Model)
    {
        <li>@item.Title<br />@item.Description</li>
    }
</ul>
